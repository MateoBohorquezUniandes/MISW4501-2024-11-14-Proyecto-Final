						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.sendRequest({\r",
									"    url: `${pm.collectionVariables.get(\"AUTH_URL\")}/auth`,\r",
									"    method: 'POST',\r",
									"    header: { 'content-type': 'application/json' },\r",
